---
layout: none
---
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title }}",
      "category" : "{{ post.categories | join: ', ' }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ post.url | relative_url }}",
      "date"     : "{{ post.date | date: '%Y년 %m월 %d일' }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]